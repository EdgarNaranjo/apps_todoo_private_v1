<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_constructor_power_bi_data_00" model="ir.sequence">
            <field name="name">Constructor Power Bi Data</field>
            <field name="code">constructor.power.bi.data</field>
            <field name="prefix">BI/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_power_bi_setting_00" model="ir.sequence">
            <field name="name">Power Bi Setting</field>
            <field name="code">power.bi.setting</field>
            <field name="prefix">SETTING/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="documents_folder_power_bi" model="documents.folder">
            <field name="name">PowerBI</field>
            <field name="group_ids" eval="[Command.link(ref('powerbi_todoo.group_power_bi_user_admin'))]"/>
            <field name="user_specific" eval="True"/>
        </record>

        <record id="documents_power_bi_status" model="documents.facet">
            <field name="name">Status</field>
            <field name="sequence">1</field>
            <field name="folder_id" ref="documents_folder_power_bi"/>
        </record>

        <record id="documents_power_bi_tag00" model="documents.tag">
            <field name="name">Created</field>
            <field name="sequence">1</field>
            <field name="facet_id" ref="documents_power_bi_status"/>
        </record>

        <record id="documents_power_bi_tag01" model="documents.tag">
            <field name="name">Updated</field>
            <field name="sequence">2</field>
            <field name="facet_id" ref="documents_power_bi_status"/>
        </record>

        <record id="documents_power_bi_tag02" model="documents.tag">
            <field name="name">Overwrite</field>
            <field name="sequence">3</field>
            <field name="facet_id" ref="documents_power_bi_status"/>
        </record>

        <record id="ir_cron_data_update_list_csv" model="ir.cron">
            <field name="name">PowerBI: Update data excel</field>
            <field name="model_id" ref="model_power_bi_setting"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.update_list_excel()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="ir_cron_read_setting_power_bi" model="ir.cron">
            <field name="name">PowerBI: Read Setting Power BI</field>
            <field name="model_id" ref="model_power_bi_setting"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.read_setting_power_bi()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
